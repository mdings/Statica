{
  "name": "statica",
  "description": "Compile a static website really easy",
  "author": "Maarten Dings <maartendings@gmail.com>",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "rebuild": "./node_modules/.bin/electron-rebuild --version=1.6.11",
    "dmg": "electron-installer-dmg ./package/statica-darwin-x64/statica.app statica",
    "dev": "cross-env NODE_ENV=development webpack --colors --watch",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "version": "electron -v",
    "start": "concurrently \"npm run electron\" \"npm run dev\"",
    "electron": "cross-env NODE_ENV=development electron .",
    "package": "npm run rebuild; npm run build; npm run package-osx;",
    "package-osx": "electron-packager ./ --electronVersion=1.6.11 --platform=darwin --arch=x64 --icon=assets/app.icns --out=package --overwrite=force --prune=true --asar",
    "package-win": "electron-packager ./ --electronVersion=1.6.11 --platform=win32 --arch=ia32 --icon=assets/app.ico --out=package --overwrite=force --prune=true --asar",
    "package-linux": "electron-packager ./ --electronVersion=1.6.11 --platform=linux --arch=ia32 --icon=assets/app.ico --out=package --overwrite=force --prune=true --asar"
  },
  "build": {
    "appId": "com.statica.www"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^3.5.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "electron-installer-dmg": "^0.2.1",
    "electron-packager": "^9.1.0",
    "electron-rebuild": "^1.6.0",
    "node-dir": "^0.1.17",
    "node-sass": "^4.6.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-csso": "^3.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "svg-url-loader": "^2.3.0",
    "uglifyjs": "^2.4.11",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "vue": "^2.5.3",
    "vue-loader": "^13.5.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.3",
    "vuex": "^3.0.1",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "babel-preset-env": "^1.6.1",
    "babelrc-rollup": "^3.0.0",
    "browser-sync": "^2.18.13",
    "coffeescript": "^2.0.2",
    "ejs": "^2.5.7",
    "electron-settings": "^3.1.4",
    "electron-store": "^1.3.0",
    "find-up": "^2.1.0",
    "ftp": "^0.3.10",
    "gh-pages": "^1.0.0",
    "gray-matter": "^3.1.1",
    "imagemin": "^5.3.1",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-pngquant": "^5.0.1",
    "keytar": "^4.0.5",
    "less": "^2.7.3",
    "markdown-it": "^8.4.0",
    "node-dir": "^0.1.17",
    "node-notifier": "^5.1.2",
    "node-sass": "^4.6.0",
    "omit-deep": "^0.3.0",
    "parse-git-config": "^1.1.1",
    "postcss": "^6.0.14",
    "pug": "^2.0.0-rc.4",
    "require-dir": "^0.3.2",
    "rollup": "^0.51.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-coffee-script": "^2.0.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "shortid": "^2.2.8",
    "stylus": "^0.54.5",
    "uglify-js": "^3.1.8",
    "upath": "^1.0.0"
  }
}
